{"objectives":
  [
  <% @objectives.each do |o| %>
  {
    "id": <%= o[:id] %>,
    "name": "<%= o[:name] %>",
    "description": "<%= o[:description] %>",
    "targetgoal": <%= o[:targetgoal] %>,
    "targetdate": "<%= o[:targetdate] %>",
    "progress": <%= o[:progress] %>,
    "pace": <%= o[:pace] %>,
    "progress_pct": <%= o[:progress_pct] %>,
    "last_progress_on": "<%= o[:last_progress_on] %>",
    "archived": <%= o[:archived] %>,
    "created_at": "<%= o[:created_at] %>",
    "updated_at": "<%= o[:updated_at] %>"
  }<%= ',' unless @objectives.last == o %>
  <% end %>
  ]
}
